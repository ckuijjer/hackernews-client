{
  "expo": {
    "name": "hackernews-client",
    "slug": "hackernews-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ckuijjer.hackernewsclient"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ckuijjer.hackernewsclient"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ebcd1e7b-e56f-42cd-bab6-ab9c53b7a4ec"
      },
      "ignoreErrorAndWarningNotifications": false
    },
    "jsEngine": "hermes",
    "plugins": ["expo-web-browser"],
    "newArchEnabled": true
  }
}
